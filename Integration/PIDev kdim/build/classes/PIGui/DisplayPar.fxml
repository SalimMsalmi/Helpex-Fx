            <TableColumn fx:id="par" prefWidth="39.0" text="Par" visible="false" />
            <TableColumn fx:id="user" prefWidth="37.0" text="User" visible="false" />
            <TableColumn fx:id="event" prefWidth="40.0" text="Event" visible="false" />
