                  <Font name="Bell MT" size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnSignOut" layoutX="514.0" layoutY="321.0" mnemonicParsing="false" onAction="#signout" opacity="0.0" prefHeight="27.0" prefWidth="48.0" text="Sign out">
